"*":
  Hydrogen:
    debug: true
    kernelNotifications: true
    outputAreaDefault: true
    startDir: "dirOfFile"
  "atom-terminal":
    setWorkingDirectory: false
    surpressDirectoryArgument: false
  "atom-terminal-panel":
    enableConsoleStartupInfo: false
    enableWindowAnimations: false
    logConsole: true
    overrideLs: false
  "autocomplete-clang":
    ignoreClangErrors: false
    includeNonDoxygenCommentsAsDocumentation: true
    includeSystemHeadersDocumentation: true
    "std c++": "c++14"
  "clang-format":
    formatCOnSave: true
    formatCPlusPlusOnSave: true
    formatJavaOnSave: true
    formatJavascriptOnSave: true
    formatObjectiveCOnSave: true
  core:
    autoHideMenuBar: true
    packagesWithKeymapsDisabled: [
      "atom-terminal"
    ]
    telemetryConsent: "limited"
  editor:
    fontSize: 15
  "exception-reporting":
    userId: "e96d1130-51bf-47f2-833a-150e2769f274"
  "gcc-make-run":
    cflags: "-Wall -Werror -Wpedantic -g"
  "gpp-compiler":
    cppCompilerOptions: "-Wall -Werror -Wpedantic"
    linuxTerminal: "GNOME Terminal"
  "hydrogen-launcher": {}
  "linter-clang":
    clangDefaultCFlags: "-Wall -Werror"
    clangDefaultCppFlags: "-Wall -Werror -Wpedantic -std=c++14"
  "linter-gcc":
    gccDefaultCppFlags: "-c -Wall -Werror -Wpedantic -std=c++14"
    gccLintOnTheFly: true
    gccSuppressWarnings: true
  "linter-ui-default":
    panelHeight: 213
    showPanel: true
  minimap:
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  "relative-numbers":
    showAbsoluteNumbers: true
    trueNumberCurrentLine: false
  "vim-mode":
    useSmartcaseForSearch: true
  zentabs:
    neverCloseDirty: true
    neverCloseNew: true
    neverCloseUnsaved: true
